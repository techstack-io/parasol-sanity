{
    "workbench.colorCustomizations": {
        "statusBar.foreground": "#96FFE7",
        "statusBar.background": "#0F7F65",
        "statusBarItem.hoverBackground": "#1B535F",
        "statusBarItem.remoteForeground": "#D4E8FF",
        "statusBarItem.remoteBackground": "#2C64A2",
        "statusBar.debuggingForeground": "#000000",
        "statusBar.debuggingBackground": "#45B69C"
    },
    "eslint.enable": false,
    "jshint.enable": false,
    "standard.enable": false,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "explorerExclude.backup": null
}